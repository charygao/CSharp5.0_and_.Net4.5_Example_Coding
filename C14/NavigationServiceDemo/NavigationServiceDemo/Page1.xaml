<Page x:Class="NavigationServiceDemo.Page1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Page1">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <StackPanel>
           <TextBlock FontSize="20" HorizontalAlignment="Center">这是第一页</TextBlock>
            <StackPanel Orientation="Horizontal">
                <Button Name="btn1" Click="btn1_Click" HorizontalAlignment="Center" Margin="10,0,0,0">打开第二页</Button>
                <Button Name="btn2" Click="btn2_Click" HorizontalAlignment="Center" Margin="10,0,0,0">前进</Button>
                <Button Name="btn3" Click="btn3_Click" HorizontalAlignment="Center" Margin="10,0,0,0">后退</Button>
                <Button Name="btn4" Click="btn4_Click" HorizontalAlignment="Center" Margin="10,0,0,0">刷新</Button>
                <Button Name="btn5" Click="btn5_Click" HorizontalAlignment="Center" Margin="10,0,0,0">停止</Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
